{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_ANDROID_NDK_ABI_FILTERS": "arm64-v8a"
      }
    },
    "debug": {
      "distribution": "internal",
      "env": {
        "EXPO_ANDROID_NDK_ABI_FILTERS": "arm64-v8a"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
